
general {
  interval = 1
  colors = true
  color_good = "#a9b1d6"
  color_degraded = "#e0af68"
  color_bad = "#f7768e"
}

order += "tztime local"
order += "disk /"
order += "wireless wlan0"

wireless wlan0 {
  format_up = " W: %quality "
  format_down = " W: down "
}

tztime local {
  format = " %H:%M:%S "
}

disk "/" {
  format = " %free "
}

